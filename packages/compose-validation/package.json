{
  "name": "compose-validation",
  "version": "5.1.0",
  "description": "Vue composition function for form validation",
  "author": {
    "name": "jens",
    "email": "jens@doellmann.com"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/compose-validation.cjs.js",
  "types": "dist/compose-validation.d.ts",
  "exports": {
    "import": "./dist/compose-validation.esm.js",
    "require": "./dist/compose-validation.cjs.js"
  },
  "files": [
    "dist/*.js",
    "dist/compose-validation.d.ts",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JensDll/vue3-form-validation"
  },
  "keywords": [
    "vue",
    "vuejs",
    "form",
    "validation",
    "utils",
    "vue-use"
  ],
  "dependencies": {
    "vue-demi": "latest",
    "@compose-validation/shared": "workspace:*"
  },
  "peerDependencies": {
    "vue": "2.x || 3.x",
    "@vue/composition-api": "^1.4.2"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
